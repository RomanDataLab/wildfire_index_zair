{
  "version": 2,
  "buildCommand": "cd outputs && npm install && npm run vercel-build",
  "outputDirectory": "outputs/dist",
  "installCommand": "cd outputs && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/methodology.html",
      "destination": "/methodology.html"
    },
    {
      "source": "/((?!methodology\\.html).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        }
      ]
    }
  ]
}

